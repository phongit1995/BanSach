
<% statusHelper= (linkPrefix, status, id) => {
    let xhtmlStatus = '';
    let link        = linkPrefix +'change-status/' + id +'/' + status;
    if(status === 'active'){
        xhtmlStatus = '<span class="label label-success">active</span>';
    } else {
        xhtmlStatus = '<span class="label label-warning">inactive</span>';
    }
   return `<a href="${link}">
        ${xhtmlStatus}
   </a>`;
}%>